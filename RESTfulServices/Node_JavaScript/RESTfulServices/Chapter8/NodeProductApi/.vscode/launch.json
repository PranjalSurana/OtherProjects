    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug the service",
            "type": "node",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\src\\controllers\\product-rest-controller.js"
        },
        {
            "name": "Debug Jasmine Spec",
            "type": "node",
            "request": "launch",
            "skipFiles": ["<node_internals>/**"],
            "program": "${workspaceRoot}\\node_modules\\jasmine\\bin\\jasmine.js",
            "args": ["${relativeFile}"]
        }  
    ]
}
